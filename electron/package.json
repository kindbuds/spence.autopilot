      "output": "build"
    },
    "files": [
      "**/*",
      "vue-app/dist/**/*"
    ]
  },
  "config": {
    "forge": {
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "Spence",
            "authors": "Spence",
            "description": "AI Career Copilot"
          }
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {
            "mimeType": [
              "x-scheme-handler/spence"
            ]
          }
        },
        {
          "name": "@electron-forge/maker-dmg",
          "config": {}
        }
      ]
    }
